{
  "build": {
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "CEO Email Explorer",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "dialog": {
        "all": true,
        "open": true,
        "save": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.example.ceoemailsearch",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [
        "../../vector_engine/src/engine_cli.py",
        "../../vector_engine/src/pst_processor.py",
        "../../vector_engine/src/data_loader.py",
        "../../vector_engine/src/db_explorer.py",
        "../../vector_engine/requirements.txt"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "CEO Email Explorer",
        "width": 800,
        "height": 600
      }
    ]
  }
}
